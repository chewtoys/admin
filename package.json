{
  "name": "admin",
  "version": "2.8.1",
  "description": "A Nuxt.js project",
  "author": "yuukit <yuma.kitamura@ponos.co.jp>",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run tailwind:w\" \"nuxt-ts\"",
    "build": "npm run tailwind:build && nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "export": "nuxt-ts export",
    "tailwind:w": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss client/assets/tailwind.scss -o client/assets/tailwind-output.scss -w",
    "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production  postcss client/assets/tailwind.scss -o client/assets/tailwind-output.scss",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .ts,.vue --ignore-path .gitignore . --fix",
    "precommit": "npm run lint",
    "test:unit": "jest"
  },
  "dependencies": {
    "@nekohack/j-stylebook": "1.1.0",
    "@nuxtjs/apollo": "4.0.1-rc.5",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/composition-api": "0.31.0",
    "@nuxtjs/device": "2.1.0",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/pwa": "3.3.5",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "chart.js": "3.7.0",
    "core-js": "3.20.2",
    "dayjs": "1.10.7",
    "firebase": "9.6.2",
    "graphql": "15.8.0",
    "graphql-tag": "2.12.6",
    "node-fetch": "2.6.6",
    "nuxt": "2.15.8",
    "nuxt-i18n": "6.28.1",
    "ts-node": "10.4.0",
    "vue-chartjs": "3.5.1",
    "vue-cookie-law": "1.13.3",
    "vue-google-charts": "0.3.3",
    "vue-no-ssr": "1.1.1",
    "vue-single-picker": "1.2.0",
